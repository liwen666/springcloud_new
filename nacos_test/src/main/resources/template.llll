spring:
  main:
    allow-bean-definition-overriding: true
  profiles:
    active: dev
  application:
    name: anyest-auth
  servlet:
    multipart:
      max-file-size: 1024MB
      max-request-size: 1024MB
  aop:
    auto: true
    proxy-target-class: true
swagger:
  docket:
    groupName: jrx.auth
    basePackage: jrx.anyest.mc

  api-info:
    title: 综管中心接口
    description: 综管中心restful文档
    license: published by jrx
    version: 3.0.0
  enable: true

auth:
  setting:
    swagger-open: true
    #是否开启日志记录，默认false
    visitLog: true
    #是否开启权限，默认
    openAuth: true
    #是否登陆加密，默认false
    secretLogin: true
    #登陆界面
    loginPage: /mc/page/login.html?logout=true
    #登陆成功后跳转界面
    default-success-url: /mc/page/manage.html#/business/table
    tokenExpTimeMinute: 60
    appResourceLoad: /
    projectPackage: AuthFileLoader
    allows:
      - /mc/user/pwd
      - /mc/user/securitycode/**
      - /error/**
base:
  test: false

server:
  port: 9600


